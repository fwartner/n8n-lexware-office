{
  "name": "n8n-nodes-lexware-office",
  "version": "1.0.0",
  "description": "n8n node for Lexware Office API integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "dev": "tsc --watch"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/LexwareOffice/LexwareOffice.node.js"
    ],
    "credentials": [
      "dist/credentials/LexwareOfficeApi.credentials.js"
    ]
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "lexware",
    "office",
    "api"
  ],
  "author": {
    "name": "Florian Wartner",
    "email": "florian@pixelandprocess.de"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
