{
  "name": "n8n-nodes-lexware-office",
  "version": "1.0.0",
  "description": "n8n node for Lexware Office API integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lint:fix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/LexwareOffice/LexwareOffice.node.js"
    ],
    "credentials": [
      "dist/credentials/LexwareOfficeApi.credentials.js"
    ]
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "lexware",
    "office",
    "api"
  ],
  "author": {
    "name": "Your Name",
    "email": "your@email.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "1.1.15",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6.4"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
