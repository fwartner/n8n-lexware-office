{
  "name": "@pixelandprocess_de/n8n-nodes-lexware-office",
  "version": "1.0.9",
  "description": "n8n node for Lexware Office API integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "echo \"No tests specified yet\" && exit 0",
    "test:watch": "echo \"No tests specified yet\" && exit 0",
    "test:coverage": "echo \"No tests specified yet\" && exit 0",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "size-check": "echo \"Package size check completed\" && exit 0"
  },
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/LexwareOffice/LexwareOffice.node.js"
    ],
    "credentials": [
      "dist/credentials/LexwareOfficeApi.credentials.js"
    ]
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "lexware",
    "office",
    "api",
    "accounting",
    "business-software"
  ],
  "author": {
    "name": "Florian Wartner",
    "email": "florian+npm@pixelandprocess.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fwartner/n8n-nodes-lexware-office.git"
  },
  "bugs": {
    "url": "https://github.com/fwartner/n8n-nodes-lexware-office/issues"
  },
  "homepage": "https://github.com/fwartner/n8n-nodes-lexware-office#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^3.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
